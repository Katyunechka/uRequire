 "version": "0.4.0",
 "homepage": "urequire.org",
 "author": {
  "name": "Agelos Pikoulas",
  "email": "agelos.pikoulas@gmail.com"
 },
 "licenses": [
  {
   "type": "MIT",
   "url": "http://www.opensource.org/licenses/mit-license.php"
  }
 ],
 "keywords": [
    "modules", "module", "modular", "format",
    "convert", "converter", "conversion",
    "commonjs", "AMD", "UMD", "UMDjs",
    "requirejs","require", "define", "umdify",
    "nodefy", "browserify", "dependency", "dependencies",
    "bundle", "package"
 ],
 "repository": {
  "type": "git",
  "url": "git://github.com/anodynos/urequire"
 },
