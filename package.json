  "minimatch": "*",
  "lodash": "*",
  "requirejs": "2.1.x",
  "uberscore": "0.0.10",
  "uglify-js": "1.3.x",
  "underscore.string":"*",
  "wrench": "1.4.x"
 },
 "devDependencies": {
  "chai": "*",
  "grunt": "0.4.1",
  "grunt-contrib-clean": "0.4.1",
  "grunt-contrib-copy": "0.4.1",
  "grunt-contrib-concat": "0.1.3",
  "grunt-shell": "0.2.2"
 }
